{
  "component_name": "modus-action-bar",
  "version": "v1",
  "props": [
    {
      "name": "actions",
      "description": "/** (optional) List of actions */",
      "type": "ModusActionBarOptions[]"
    },
    {
      "name": "size",
      "description": "/** (optional) List of actions */",
      "type": "'small' | 'medium' | 'large'"
    },
    {
      "name": "visibleItemCount",
      "description": "/** (optional) List of actions */",
      "type": "this.actions.slice(0, this.visibleItemCount - 1) : this.actions"
    }
  ],
