    "actions": "actionItems.map((item) => ({\n        id: item.getAttribute('id'),\n        icon: item.getAttribute('icon'),\n        label: item.textContent.trim(),\n      }))",
    "size": "'medium'",
    "visibleItemCount": "3"
  },
